@model Contact

@{
    ViewData["Title"] = "Detail";
}


<div class="container my-5">

    <h1 style="color:darkcyan">Contact detail</h1>

    <div class="my-3">

        <h4 class="my-3">Full Name</h4>
        <input class="form-control" id="customFile" asp-for="FullName" disabled="disabled" />

        <h4 class="my-3">Email</h4>
        <input type="text" class="form-control" id="customFile" asp-for="Email" disabled="disabled" />

        <h4 class="my-3">Subject</h4>
        <input class="form-control" id="customFile" asp-for="Subject" disabled="disabled" />

        <h4 class="my-3">Message</h4>
        <input class="form-control" id="customFile" asp-for="Message" disabled="disabled" />

    </div>
    <a class="btn btn-info mt-3" asp-action="Index">
        Back
    </a>

</div>

